<%- headers = ['Order#', 'Status', 'Total Price', 'Created At', 'Customer', 'Company', 'Admin Assigned'] -%>
<%= CSV.generate_line headers %>
<%- @companies_orders.each do |orders| -%>
<%= CSV.generate_line([orders.id, orders.status, orders.total_price, orders.created_at.to_date,orders.user.forname + " " + orders.user.surname, orders.company.name, orders.taken_by]) -%>
<%- end -%>
