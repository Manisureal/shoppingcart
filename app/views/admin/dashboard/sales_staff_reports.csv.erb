<%- headers = ['Order#', 'Status', 'Total Price', 'Created At', 'Customer', 'Company', 'Admin Assigned'] -%>
<%= CSV.generate_line headers %>
<%- @sales_staff_companies_orders.each do |orders| -%>
<%= CSV.generate_line([orders.id, orders.status, orders.total_price, orders.created_at.strftime("%A, %b %d - %H:%M:%S"),orders.user.forname + " " + orders.user.surname, orders.company.name, orders.taken_by]) -%>
<%- end -%>
